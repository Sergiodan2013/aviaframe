      "name": "DRCT Search API",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.1,
      "position": [470, 300]
    },
    {
      "parameters": {
        "jsCode": "// Transform DRCT API response\nconst response = $input.item.json;\n\n// Extract search results\nconst offers = response.offers || [];\n\n// Transform each offer to our format\nconst transformedOffers = offers.map(offer => ({\n  id: offer.id,\n  price: {\n    total: offer.price?.total || 0,\n    currency: offer.price?.currency || 'RUB',\n    base: offer.price?.base || 0,\n    taxes: offer.price?.taxes || 0\n  },\n  segments: offer.segments?.map(segment => ({\n    origin: segment.origin,\n    destination: segment.destination,\n    departure: segment.departure,\n    arrival: segment.arrival,\n    carrier: segment.carrier,\n    flight_number: segment.flight_number,\n    aircraft: segment.aircraft,\n    duration: segment.duration\n  })) || [],\n  baggage: offer.baggage || {},\n  fare_family: offer.fare_family || 'ECONOMY',\n  available_seats: offer.available_seats || 0\n}));\n\nreturn {\n  search_id: response.search_id || 'unknown',\n  offers: transformedOffers,\n  metadata: {\n    total_offers: transformedOffers.length,\n    timestamp: new Date().toISOString(),\n    api_version: response.api_version || '1.0'\n  }\n};"
      },
      "id": "c5d6e7f8-9a0b-1c2d-3e4f-5a6b7c8d9e0f",
